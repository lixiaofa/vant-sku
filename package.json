{
  "name": "vant-sku",
  "version": "1.0.2",
  "packageManager": "pnpm@7.14.0",
  "description": "migration from vant sku",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vant-sku.umd.cjs",
  "module": "./dist/vant-sku.es.js",
  "exports": {
    ".": {
      "import": "./dist/vant-sku.es.js",
      "require": "./dist/vant-sku.umd.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "cross-env MOCK_SERVER_PORT=8086 vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "plop": "plop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lixiaofa/vant-sku.git",
    "directory": ""
  },
  "bugs": "https://github.com/lixiaofa/vant-sku/issues",
  "author": "faxiaoli",
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "mobile",
    "frontend",
    "component"
  ],
  "homepage": "https://github.com/lixiaofa/vant-sku",
  "dependencies": {
    "lodash": "^4.17.21",
    "tiny-emitter": "^2.1.0",
    "vant": "^4.0.2",
    "vant-sku": "^1.0.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.12",
    "@types/nprogress": "^0.2.0",
    "@types/store": "^2.0.2",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@yuo/postcss-px2vw": "^1.0.5",
    "autoprefixer": "^10.4.13",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.6",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "cz-emoji-chinese": "^0.3.1",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.1",
    "plop": "^3.1.1",
    "rollup": "^3.7.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "signale": "^1.4.0",
    "slash2": "^2.0.0",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^4.0.0",
    "vite-plugin-vconsole": "^1.2.2",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-chinese"
    },
    "cz-emoji-chinese": {
      "skipQuestions": [
        "body",
        "scope"
      ]
    }
  }
}
